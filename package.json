  "main": "index.js",
  "types": "index.d.ts",
  "authors": [
    "Kas Elvirov <kas.elvirov@gmail.com> (https://github.com/kas-elvirov)"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn version-patch:up",
      "pre-push": "yarn test"
    }
