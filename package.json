    "version-major:up": "npm version major --no-git-tag-version --force && git add package.json package-lock.json"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.20",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "babel-minify": "^0.5.1",
    "expect": "^26.6.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
